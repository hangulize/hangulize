    "vl"     = "c", "C", "f", "k", "p", "q", "s", "S", "t", "T", "x"
    "vo"     = "b", "d", "D", "g", "l", "m", "n", "N", "r", "v", "w", "z", "Z"
    "vowels" = "a", "e", "i", "o", "u", "ü"

normalize:
    "ç" = "Ç"
    "ü" = "Ü"
    "·" = "•"

rewrite:
    "felix$"         -> "felics"
    "helix$"         -> "helics"
