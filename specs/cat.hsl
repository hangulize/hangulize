lang:
    id      = "cat"
    codes   = "ca", "cat"
    english = "Catalan"
    korean  = "카탈로니아어"
    script  = "latin"

config:
    author = "Brian Jongseong Park <iceager@gmail.com>"
    stage  = "draft"

macros:
    "@" = "<vowels>"

vars:
    "cs"     = "b", "c", "C", "d", "D", "f", "g", "h", "k", "l", "m", "n", "N", "p", "r", "s", "S", "t", "T", "v", "w", "x", "z", "Z"
